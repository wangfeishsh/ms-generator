		<!--生成Dao类存放位置 targetPackage 需要自定义 目录自动生成 basePackage-->
		<javaClientGenerator targetPackage="pay.dao.local.db2" targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 正式生成之前需要删除 此事例 -->
